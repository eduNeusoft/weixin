	left:0;
	z-index:1;
}
.card .logo {
	max-height:70px;
	position:absolute;
	top:8px;
	left:5px;
	z-index:2;
}
#card .msk{
	display: inline-block;
    position: absolute;
	width:320px;
	height:115px;
	top:78px;
	left:0;
	z-index:3;
	-webkit-background-size:320px 128px;
	background-size:320px 128px;
}
#card .card .right-top {
	position:absolute;
	right:10px;
	top:12px;
	z-index:2;
}
#card .card .avater {
	position:absolute;
	left:15px;
	bottom:12px;
	z-index:2;
}
#card .card .info {
	position:absolute;
	left:75px;
	bottom:15px;
	z-index:2;
}
#card .card .right-top div{
